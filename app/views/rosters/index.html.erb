<p id="notice"><%= notice %></p>

<h1>Rosters</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Email</th>
      <th>Cell phone</th>
      <th>Home phone</th>
      <th>Work phone</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rosters.each do |roster| %>
      <tr>
        <td><%= roster.first_name %></td>
        <td><%= roster.last_name %></td>
        <td><%= roster.email %></td>
        <td><%= roster.cell_phone %></td>
        <td><%= roster.home_phone %></td>
        <td><%= roster.work_phone %></td>
        <td><%= link_to 'Show', roster %></td>
        <td><%= link_to 'Edit', edit_roster_path(roster) %></td>
        <td><%= link_to 'Destroy', roster, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Roster', new_roster_path %>
